{
  "name": "sij-core",
  "version": "0.0.2",
  "description": "Embedding of SQL-92 into Javascript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    ">=3.1": {
      "*": [
        "*"
      ],
      "ast": [
        "dist/ast/index.d.ts"
      ],
      "builder": [
        "dist/builder/index.d.ts"
      ],
      "render": [
        "dist/render/index.d.ts"
      ],
      "util": [
        "dist/util/index.d.ts"
      ]
    }
  },
  "exports": {
    "./": "./dist/"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npx tsc && copyfiles -u 2 \"src/types/*.d.ts\" dist",
    "format": "npx prettier . --write",
    "test": "ava -v && tsd",
    "docs": "npx typedoc src/ --out docs/ --includeDeclarations --excludeExternals --excludeNotExported --excludePrivate && touch docs/.nojekyll"
  },
  "keywords": [
    "sql"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "helpers": [
      "**/util.ts*"
    ],
    "timeout": "20s"
  },
  "author": "Claire Neveu",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "ava": "^6.1.1",
    "copyfiles": "^2.4.0",
    "eslint": "^8.56.0",
    "prettier": "3.2.5",
    "ts-node": "^10.9.2",
    "tsd": "^0.30.4",
    "typedoc": "^0.25.8",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "callable-instance": "^2.0.0",
    "lens.ts": "^0.5.1",
    "ts-toolbelt": "^9.6.0"
  }
}
